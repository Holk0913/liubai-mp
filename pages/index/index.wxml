<view class="container">
    <view class="header">
        <image src="/images/code.jpeg" class="header-icon"></image> 
        <text>IIU UI</text>
    </view>
    <view class="body">
        <navigator wx:for="{{ list }}" url="{{ item.route }}" wx:key="this" class="item">
              <text class="item-text">{{ item.label }}</text>
              <view class="icon-wrap"><image src="data:image/png;base64," class="icon-arrow"/></view>
        </navigator>
    </view>
</view>