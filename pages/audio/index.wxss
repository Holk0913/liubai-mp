page{background-image: linear-gradient(to bottom, #195684, #323435)}
.container{position: fixed; left: 0; top: 0; bottom: 0; right: 0; }
.scroll-box{width: 100%;  position: fixed; top: 400rpx; left: 0; right: 0; border-top: 1rpx solid  rgba(0,0,0,0.1); margin-bottom: 150rpx;}
.slist{margin-bottom: 60px;}
.list-item-box{display: flex; position:relative; flex-direction: column; align-content: flex-start; justify-content: center; color: #fff; box-sizing: border-box; padding: 20rpx 40rpx; border-bottom:  1rpx solid  rgba(0,0,0,0.1)}
.list-item-box:last-child{border-bottom: none;}
.list-item-bottom{display: flex; flex-direction: row; justify-content: flex-end; align-items: center;}
.list-item-box .title{flex-direction: row; justify-content: space-between; align-items: center; white-space:nowrap;overflow:hidden;text-overflow:ellipsis;  flex: 0 0 480rpx;}
.list-item-box .playing-title{color: #02d7f4;}
.list-item-box .author{font-size: 24rpx; color: #f2f2f2; margin-right: 40rpx;display: flex; justify-content: flex-start; align-items: center;}
.list-item-box .duration, .list-item-box .date{font-size: 24rpx;color: #f2f2f2;display: flex; justify-content: flex-start; align-items: center;}
.list-item-box .date{margin-left: 30rpx;}
.list-item-top{display: flex; flex-direction: row; justify-content: space-between; align-items: center;font-size: 30rpx; padding-top: 20rpx; padding-bottom: 20rpx;}
.play-animation-box{}
.play-animation-box .inner{width: 40rpx; height: 22rpx;overflow: hidden; display: flex; justify-content: flex-start; justify-items: center;}
.play-animation-box .inner .line{width: 2rpx; height: 22rpx; margin-right: 4rpx; background-color: #02d7f4; animation:bounce ease-in-out .7s forwards infinite;}
.play-animation-box .inner .line:nth-of-type(1) {animation-duration: .4s;}
.play-animation-box .inner .line:nth-of-type(2) {animation-duration: .6s;}
.play-animation-box .inner .line:nth-of-type(3) {animation-duration: .7s;}
.play-animation-box .inner .line:nth-of-type(4) {animation-duration: .5s;}
.playing-title{margin-right: 30rpx;}
.list-item-top{ margin-bottom: 5px;}
.progress{position: absolute; width: 0; height: 1px; background: #02d7f4; bottom: 0; left: 0;}

.play-area{position: absolute; top: 0; left: 0; right: 0; height: 400rpx; display: flex; justify-content: center; align-items: center;}
.play-area .play, .play-area .stop{position: absolute; left: 20rpx; top: 20rpx; width: 60rpx; height: 60rpx; transition: .4s ease-in-out;}
.play-area .stop{transform: translate3d(0, 0, 0); opacity: 1; background: url(data:image/png;base64,) no-repeat; background-size: cover;}
.play-area .play{transform: translate3d(100%, 0, 0); opacity: 0; background: url(data:image/png;base64,) no-repeat; background-size: cover;}
.play-area .btn{width: 100rpx; height: 100rpx; position: relative; overflow: hidden;}
.foot-box{padding: 20rpx 0; justify-content: center; align-items: center; display: flex; color: #fff; font-size: 24rpx;}
.weui-loading{margin:0 5px;width:20px;height:20px;display:inline-block;vertical-align:middle;-webkit-animation:weuiLoading 1s steps(12,end) infinite;animation:weuiLoading 1s steps(12,end) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;background-size:100%}

.share-btn{position: absolute; right: 30rpx; top: 30rpx; width: 60rpx; height: 60rpx; background: url(data:image/png;base64,) no-repeat; background-size: cover;}
 .share-btn-inner{position: absolute; width: 100%; height: 100%; opacity: 0; z-index: 20;}
 .article-btn{background: url(data:image/png;base64,) no-repeat; background-size: cover; position: absolute; width: 56rpx; height: 56rpx; left: 30rpx; top: 30rpx;}
 .icon-link{width: 28rpx; height: 28rpx;margin-right: 10rpx;}
 .article{display: flex; justify-content: center; flex-direction: row; align-items: center; flex: 0 0 100rpx; }
 .slot-box{ height: 100%;display: flex; justify-content: center; align-items: center; font-size: 24rpx; color: #fff; padding-right: 20rpx;}
.footer-tab{display: flex; justify-content: space-around; align-items: center; position: fixed; bottom: 0; left: 0; right: 0; padding: 20rpx 0;}
.footer-tab-item{font-size: 24rpx; color: #fff; position: relative;}
.footer-tab-item .shadow{position: absolute; width: 36px; height: 12px;opacity:0.3; background-color: #fff; filter: blur(4px);left: 50%; top: 50%;  transform: translate3d(-50%, -50%,0); z-index: 20;}
.icon-author, .icon-time, .icon-date, .icon-langtype{width: 28rpx; height: 28rpx; margin-right: 12rpx;}
.icon-author{background: url(data:image/png;base64,) no-repeat; background-size: cover;}
.icon-time{background: url(data:image/png;base64,) no-repeat; background-size: cover;} 
.icon-date{background: url(data:image/png;base64,) no-repeat; background-size: cover;}
.icon-langtype{background: url(data:image/png;base64,) no-repeat; background-size: cover;}